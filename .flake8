[flake8]
# E203: Whitespace before ':'.
# W503: Line break before binary operator.
# E501: Line too long. Max-line-length instead.
ignore = E203, E501, W503, E712

max-line-length = 88

max-complexity = 10

exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    alembic/versions